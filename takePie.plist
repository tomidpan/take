<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<plist version="1.0"> 
  <dict> 
    <!-- array of downloads. -->  
    <key>items</key>  
    <array> 
      <dict> 
        <!-- an array of assets to download -->  
        <key>assets</key>  
        <array> 
          <!-- software-package: the ipa to install. -->  
          <dict> 
            <key>kind</key>  
            <string>software-package</string>  
            <key>url</key>  
            <string>http://pie.takecn.cn/app/ios/takepie_release_last.ipa</string> 
          </dict>  
          <!-- display-image: the icon to display during download. -->  
          <dict> 
            <key>kind</key>  
            <string>display-image</string>  
            <!-- optional. icon needs shine effect applied. -->  
            <key>needs-shine</key>  
            <true/>  
            <key>url</key>  
            <string>http://pie.takecn.cn/app/ios/takepie_57.png</string> 
          </dict>  
          <!-- full-size-image: the large 512Ã—512 icon used by iTunes. -->  
          <dict> 
            <key>kind</key>  
            <string>full-size-image</string>  
            <key>needs-shine</key>  
            <true/>  
            <key>url</key>  
            <string>http://pie.takecn.cn/app/ios/takepie_512.png</string> 
          </dict> 
        </array>
        <key>metadata</key>  
        <dict> 
          <key>bundle-identifier</key>  
          <string>com.take.TakePai</string>  
          <key>bundle-version</key>  
          <string>1.0.0</string>  
          <key>kind</key>  
          <string>software</string>  
          <key>subtitle</key>  
          <string>takepie</string>  
          <key>title</key>  
          <string>takepie</string> 
        </dict> 
      </dict> 
    </array> 
  </dict> 
</plist>
